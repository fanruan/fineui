    "autoprefixer": "9.6.1",
    "babel-loader": "8.0.6",
    "chai": "4.2.0",
    "cnpm": "6.1.1",
    "concat": "1.0.3",
    "core-js": "3.3.2",
    "cross-env": "6.0.0",
    "css-loader": "3.0.0",
    "es6-promise": "4.2.8",
    "eslint": "6.0.1",
    "expose-loader": "0.7.5",
